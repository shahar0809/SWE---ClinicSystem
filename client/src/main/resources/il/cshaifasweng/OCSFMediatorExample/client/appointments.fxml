<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="il.cshaifasweng.OCSFMediatorExample.client.AppointmentsController"
            prefHeight="400.0" prefWidth="600.0">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="15.0"/>
    </padding>
    <Label alignment="CENTER" text="Available Appointments"/>
    <ListView fx:id="listView"/>
    <Label alignment="CENTER" text="Operating Hours"/>
    <HBox alignment="CENTER" spacing="5.0">
        <Button mnemonicParsing="false" onAction="#onReserve" text="Reserve"/>
    </HBox>
</AnchorPane>
